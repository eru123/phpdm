<?php

require __DIR__ . '/autoload.php';

use Wyue\Commands\CLI;
use Wyue\Database\{
    MySqlMakeMigration,
    MySqlMakeModel,
    MySqlMigrate,
    MySqlRollback
};
use App\Commands\{
    NginxAccess,
    Entrypoint
};

CLI::register([
    MySqlMakeMigration::class,
    MySqlMakeModel::class,
    MySqlMigrate::class,
    MySqlRollback::class,
    NginxAccess::class,
    Entrypoint::class,
]);

CLI::listen();
